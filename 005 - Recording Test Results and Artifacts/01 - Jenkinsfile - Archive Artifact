pipeline {
    agent { 
		docker { 
			image 'alpine:latest' 
		}
	}
    stages {
        stage('build') {
            steps {
	    	sh 'apk add --no-cache curl'
                sh "curl -o www.google.com"
		archiveArtifacts artifacts: 'google_html.html'
            }
        }
    }
}
